# Task-submit-proofig Cloud Run
# Copy to .env and set values. .env is gitignored.

# GCP (required for build.sh / deploy.sh)
GCP_PROJECT=your-gcp-project-id
GCP_REGION=us-central1

# Local dev port
PORT=8080

# Proofig API (required at runtime when processing submit tasks)
PROOFIG_API_BASE_URL=https://api.proofig.com
